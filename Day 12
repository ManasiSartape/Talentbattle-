//Write a program to find Sum of digits of a number

import java.util.*;
public class Main
{
public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
int num1=0;
int num2=0;
while(num>0){
 num1=num%10;
 num2=num2+num1;
 num=num/10;
}
System.out.println("The output is: "+num2);
}
}
